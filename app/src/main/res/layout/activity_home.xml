<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/defaultContainer"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="models.Exercise"
    android:orientation="vertical"
    android:background="@color/light_black"
>
    <LinearLayout
        android:id="@+id/teste"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/defaultContainer">
        <LinearLayout
            android:id="@+id/defaultLayout"
            style="@style/linearLayoutExercise"
            >
            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/supino_jgif"
                style="@style/ImageViewExercise"
                android:src="@drawable/muscle_group_abs"
                />

            <TextView
                android:text="SUPINO RETO"
                style="@style/TextViewExercise"
                />
        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:id="@+id/default_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="9dp"
        app:layout_constraintTop_toBottomOf="@id/scrollView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:gravity="center"    >
        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/home_button"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/image_circle_background"
            ></de.hdodenhof.circleimageview.CircleImageView>
        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/exercise"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/image_circle_background"
            ></de.hdodenhof.circleimageview.CircleImageView>
        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/calendar"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/image_circle_background"
            ></de.hdodenhof.circleimageview.CircleImageView>
    </LinearLayout>

