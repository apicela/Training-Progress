<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/defaulwt"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/light_black"
    android:theme="@style/Theme.Training">
    <LinearLayout
        style="@style/linearLayoutExercise"
        >
        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/chest_image"
            style="@style/ImageViewExercise"
            android:src="@drawable/muscle_group_abs"
            />

        <TextView
            android:id="@+id/chest_text"
            android:text="SUPINO RETO"
            style="@style/TextViewExercise"
            />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

